{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"lib": ["dom", "dom.iterable", "esnext"],
		"baseUrl": "./",
		"jsx": "react-jsx",
		"paths": {
			"@/interfaces/*": ["src/interfaces/*"],
			"@/ui/*": ["src/components/ui/*"],
			"@/containers/*": ["src/components/containers/*"],
			"@/data/*": ["src/data/*"],
			"@/utils/*": ["src/utils/*"]
		},
		"typeRoots": ["./node_modules/@types", "./@types"]
	}
}
