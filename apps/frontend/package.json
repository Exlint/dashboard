{
	"name": "@exlint.io/frontend",
	"version": "1.0.0",
	"private": true,
	"description": "Frontend package",
	"author": "Exlint <dev@exlint.io> (https://exlint.io)",
	"license": "UNLICENSED",
	"scripts": {
		"postinstall": "pnpm prisma-gen",
		"start:dev": "vite -c ./vite.config.ts",
		"start:dev:docker": "vite -c ./vite.config.ts",
		"build": "vite build -c ./vite.config.ts",
		"build:automation": "AUTOMATION=true vite build -c ./vite.config.ts",
		"preview:automation": "AUTOMATION=true vite preview -c ./vite.config.ts",
		"prisma-gen": "prisma generate --schema ../../prisma/schema.prisma",
		"lint": "eslint -c ./.eslintrc.cjs --ignore-path ./.eslintignore \"./**/*.{ts,tsx,cjs}\"",
		"type-check": "tsc --project ./tsconfig.json",
		"depcheck": "depcheck --config ./.depcheckrc.json",
		"stylelint": "stylelint --config ./stylelint.config.cjs \"./src/**/*.scss\"",
		"template:page": "hygen component new-page",
		"template:container": "hygen component new-container",
		"template:layout": "hygen component new-layout",
		"template:ui": "hygen component new-ui"
	},
	"dependencies": {
		"@codemirror/lang-javascript": "6.1.9",
		"@codemirror/lang-json": "6.0.1",
		"@codemirror/language": "6.8.0",
		"@codemirror/legacy-modes": "6.3.2",
		"@lezer/highlight": "1.1.6",
		"@prisma/client": "4.15.0",
		"@reduxjs/toolkit": "1.9.5",
		"@rjsf/core": "5.8.2",
		"@rjsf/utils": "5.8.2",
		"@rjsf/validator-ajv8": "5.8.2",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.6",
		"@uiw/codemirror-themes": "4.21.7",
		"@uiw/react-codemirror": "4.21.7",
		"bootstrap": "3.4.1",
		"date-fns": "2.30.0",
		"i18next": "22.5.1",
		"nprogress": "0.2.0",
		"react": "18.2.0",
		"react-datepicker": "4.12.0",
		"react-dom": "18.2.0",
		"react-i18next": "12.3.1",
		"react-redux": "8.0.7",
		"react-router-dom": "6.12.1",
		"react-scroll": "1.8.9",
		"react-switch": "7.0.0",
		"react-textarea-autosize": "8.4.1",
		"swr": "2.1.5"
	},
	"devDependencies": {
		"@exlint.io/common": "workspace:*",
		"@total-typescript/ts-reset": "0.4.2",
		"@types/node": "18.15.12",
		"@types/nprogress": "0.2.0",
		"@types/react": "18.2.14",
		"@types/react-datepicker": "4.11.2",
		"@types/react-dom": "18.2.6",
		"@types/react-router-dom": "5.3.3",
		"@types/react-scroll": "1.8.7",
		"@typescript-eslint/eslint-plugin": "5.59.9",
		"@typescript-eslint/parser": "5.59.9",
		"@vitejs/plugin-react": "4.0.2",
		"depcheck": "1.4.3",
		"eslint": "8.42.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-deprecation": "1.4.1",
		"eslint-plugin-i18next": "6.0.3",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-unicorn": "47.0.0",
