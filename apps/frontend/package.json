		"stylelint-config-standard-scss": "8.0.0",
		"stylelint-declaration-strict-value": "1.9.2",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vite-plugin-react-remove-attributes": "1.0.3",
		"vite-plugin-sass-dts": "1.3.6",
		"vite-tsconfig-paths": "4.2.0"
	},
	"peerDependencies": {
		"@types/node": "18.15.12",
