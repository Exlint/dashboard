		"stylelint-config-standard-scss": "8.0.0",
		"stylelint-declaration-strict-value": "1.10.7",
		"typescript": "5.7.3",
		"vite": "5.4.19",
		"vite-plugin-react-remove-attributes": "1.0.3",
		"vite-plugin-sass-dts": "1.3.30",
		"vite-tsconfig-paths": "4.3.2"
	},
	"peerDependencies": {
		"@types/node": "20.17.13",
